<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <meta http-equiv="X-UA-Compatible" content="IE=8" />
    <link rel="shortcut icon" href="@Url.Content("~/Content/favicon.ico")"/>
    <link rel="Stylesheet" type="text/css" href="@Url.Content("~/Content/PageStyle.css")" />
</head>
<body>
<table class="ContainerTable">
    <tr>
        <td align="center" valign="top">
            <table class="NavigateTable" cellpadding="0" cellspacing="4">
                <tr>
                    <td class="NavigateHeader" colspan="6" align="left">
                        <img src="@Url.Content("~/Content/GacLib_LogoAndTitle.gif")" alt="GacLib -- GPU Accelerated C++ User Interface"/>
                    </td>
                </tr>
                <tr>
                    <td align="center" valign="middle">
                        <a class="MenuButton HomeButtonSelected" href="@Url.Action("Index")">
                            HOME
                        </a>
                    </td>
                    <td align="center" valign="middle">
                        <a class="MenuButton GettingStartButton" href="@Url.Action("GettingStart")">
                            GETTING START
                        </a>
                    </td>
                    <td align="center" valign="middle">
                        <a class="MenuButton DemosButton" href="@Url.Action("Demos")">
                            DEMOS
                        </a>
                    </td>
                    <td align="center" valign="middle">
                        <a class="MenuButton DownloadButton" href="@Url.Action("Download")">
                            DOWNLOAD
                        </a>
                    </td>
                    <td align="center" valign="middle">
                        <a class="MenuButton DocumentButton" href="./StaticHtmlDoc/reference_gacui.html">
                            DOCUMENT
                        </a>
                    </td>
                    <td align="center" valign="middle">
                        <a class="MenuButton ContactButton" href="@Url.Action("Contact")">
                            CONTACT
                        </a>
                    </td>
                </tr>
                @RenderBody()
            </table>
        </td>
    </tr>
</table>
<div class="WebSiteID">京ICP备12020020号-1</div>
</body>
</html>
